package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.events.KeyboardEvent;	import flash.events.Event;	/*Class RoomAccueil 		Elle implémente toute les méthodes et attribu d'une Room Classic ,		mais elle affiche ces propre objet de decord. 				Les objets de la map sont initialement stocké dans la partie graphique 		et sont nommé comme des attribus , puis on va les rajouter dans la partie 		code pour pouvoir les manipuler.	*/	public class RoomAccueil extends ClassicRoom{				public function RoomAccueil(f:Fenetre) {			this.fenetre=f;						//On ajoute tous les objets du décord dans le tableau d'affichage			for( i=0 ;i<10;i++){				tabAffiche.push(this["objet"+(i+1)]);			}			//On tri les élément du décord par rapport à leurs position y			this.TriAff();		}	}	}