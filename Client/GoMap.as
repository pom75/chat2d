package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;	/*Class GoMap		Boutton qui permet d'allez dans la map (carte des Rooms)	*/	public class GoMap extends MovieClip {		public var fenetre:Fenetre;				public function GoMap(f:Fenetre) {			this.fenetre=f;		}				public function afficherThis(){			this.addEventListener(MouseEvent.CLICK , toMap); //Click sur le bouton d'envois de message			fenetre.addChild(this);		}				public function effacerThis(){			fenetre.removeChild(this);			this.removeEventListener(MouseEvent.CLICK , toMap);			fenetre.goMap=null;		}				//Envois au serveur qu'on va dans cette room		public function toMap(evt:MouseEvent){			fenetre.scene.connexion.sendText("CROOM=1");		}					}	}